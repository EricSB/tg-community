      [\fB\-s\fR|\fB\-\-syntax\fR=\fIatt\fR|\fIintel\fR]
      [\fB\-f\fR|\fB\-\-format\fR=\fIfmtspec\fR]
      [\fB\-o\fR|\fB\-\-output\fR=\fIfilename\fR]
      [\fB\-m\fR|\fB\-\-map\fR=\fImapspec\fR]
      [\fB\-b\fR|\fB\-\-bytes\fR=\fIstring\fR]
      [\fB\-B\fR|\fB\-\-bfd\fR[=\fIbfdspec\fR\]
      [\fB\-\-dry\-run\fR]
      \fIobjfile\fR...
